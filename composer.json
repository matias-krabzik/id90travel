{
  "name": "id/travel",
  "type": "project",
  "description": "PHP Challenge",
  "keywords": ["challenge", "id90travel"],
  "autoload": {
    "psr-4": {
      "IdTravel\\Challenge\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  },
  "require": {
    "php": "^8.0",
    "philo/laravel-blade": "3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
