{
  "name": "id/travel",
  "type": "project",
  "description": "PHP Challenge",
  "keywords": ["challenge", "id90travel"],
  "autoload": {
    "psr-4": {
      "IdTravel\\Challenge\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  },
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "jenssegers/blade": "^1.4",
    "ext-http": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "symfony/process": "^6.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
